import java.util.*;

class Job{
	public String name;
	public int time;
	public Job(String s, int time){
		
		this.time = time;
		name = new String(s);
		System.out.println("Added Job : " + name + " time : " + time);
	
	}
	
	public void display(){
		
		//Print job name
		System.out.println("Job executing : " + name + " time : " + time);
	
	}
}

