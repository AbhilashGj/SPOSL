import java.util.*;

class Job{
	public String name;
	public int time;
	public Job(String s, int time){
		
		this.time = time;
		name = new String(s);
		System.out.println("Added Job : " + name + " time : " + time);
	
	}
	
	public void display(){
		
		//Print job name
		System.out.println("Job executing : " + name + " time : " + time);
	
	}
}

class Scheduler{
	
	PriorityQueue<Job> pq;
	
	public Scheduler(){
		
		Comparator<Job> JobComp = new Comparator<Job>(){

			public int compare(Job o1, Job o2) {
				return -(o1.time - o2.time);
			}
		};
		
		pq = new PriorityQueue(JobComp);
	}
	
	public boolean addJob(String name, int time){
		
		try{
			pq.add(new Job(name, time));
			
		}
		catch(Exception e){
			
			return false;
		}
		
		return true;
	}
	
	public boolean execute(){
		
		try{
			
			
		}
		catch(Exception e){
			
		}
		
		return true;
	}
	
}

public class mainFile {

}
