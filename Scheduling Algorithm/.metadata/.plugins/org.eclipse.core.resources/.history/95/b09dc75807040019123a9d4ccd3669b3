import java.util.*;

public class mainFile {
	
	public static void main(String args[]){
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter number of processes: ");
		int n = sc.nextInt();
		int bt[] = new int[n];
		int wt[] = new int[n];
		int rem_bt[] = new int[n];
		int t = 0;
		int quantum = 2;
		
		// Fill rem_bt with bt
		for(int i = 0;i < n;i ++){
			rem_bt[i] = bt[i];
		}
	}

}
