import java.util.*;

class Job implements Comparable{
	public int pid, at, bt;
	
	public Job(int pid, int at, int bt){
		this.pid = pid;
		this.at = at;
		this.bt = bt;
	}

	public int compareTo(Object o) {
		Job j = (Job) o;
		return at - j.at;
	}
	
	public void display(){
		System.out.println(pid + "\t" + at + "\t" + bt);
	}
	
}

public class mainFile {

	public static void main(String args[]){
		
		Scanner sc = new Scanner(System.in);
		
		LinkedList<Job> q = new LinkedList<Job>();
		for(int i = 1;i < 4;i ++){
			System.out.println("For pid = " + i + "enter at and bt: ");
			q.add(new Job(i, sc.nextInt(), sc.nextInt()));
		}
		Collections.sort(q);
		for(Job j : q){
			System.out.println(j.pid + "\t" + j.at + "\t" + j.bt);
		}
	}
}
